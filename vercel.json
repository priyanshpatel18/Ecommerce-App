{
  "version": 2,
  "name": "shophub",
  "builds": [{ "src": "server/index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/products/(.*)", "dest": "/server/index.js" },
    { "src": "/user/(.*)", "dest": "/server/index.js" },
    { "src": "/vendor/(.*)", "dest": "/server/index.js" },
    { "src": "/(.*)", "dest": "/server/index.js" }
  ]
}